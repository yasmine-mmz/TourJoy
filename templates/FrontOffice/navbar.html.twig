		<link
		rel="shortcut icon" href="img/fav.png">
		<!-- Author Meta -->
		<meta
		name="author" content="colorlib">
		<!-- Meta Description -->
		<meta
		name="description" content="">
		<!-- Meta Keyword -->
		<meta
		name="keywords" content="">
		<!-- meta character set -->
		<meta
		charset="UTF-8">
		<!-- Site Title -->
		<title>TourJoy</title>
		{% block stylesheets %}
			<link rel="stylesheet" href="{{ asset('frontoffice/css/linearicons.css') }}">
			<link rel="stylesheet" href="{{ asset('frontoffice/css/font-awesome.min.css') }}">
			<link rel="stylesheet" href="{{ asset('frontoffice/css/bootstrap.css') }}">
			<link rel="stylesheet" href="{{ asset('frontoffice/css/magnific-popup.css') }}">
			<link rel="stylesheet" href="{{ asset('frontoffice/css/animate.min.css') }}">
			<link rel="stylesheet" href="{{ asset('frontoffice/css/owl.carousel.css') }}">
			<link rel="stylesheet" href="{{ asset('frontoffice/css/main.css') }}">
			<link rel="preconnect" href="https://fonts.googleapis.com">
			<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
			<link href="https://fonts.googleapis.com/css2?family=Merriweather:ital,wght@0,300;0,400;0,700;0,900;1,300;1,400;1,700;1,900&display=swap" rel="stylesheet">

		{% endblock %}
		{% set user = app.user %}
		{% set isVerified = user and not user.isVerified() %}

	</head>
	<body>

		{% block navbar %}


			<!-- start banner Area -->
			<section
				class="banner-area" id="home">
				<!-- Start Header Area -->
				<header class="default-header">
					<nav class="navbar navbar-expand-lg  navbar-light bg-secondary">
						<div class="container">
							<a class="navbar-brand" href="{{ path('app_test')}}">
								<img src="img/logo.png" alt="">
							</a>
							<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
								<span class="text-white lnr lnr-menu"></span>
							</button>
							<div class="collapse navbar-collapse justify-content-end align-items-center" id="navbarSupportedContent">
								<ul class="navbar-nav">
									<li>
										<a href="#home">Home</a>
									</li>
									<li>
										<a href="#about">About</a>
									</li>


									<li class="dropdown">
										<a class="dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
											Trip
										</a>
										<div class="dropdown-menu">
											<a class="dropdown-item" href="{{ path('showFac') }}">Accommodations</a>
											<a class="dropdown-item" href="{{ path('all_monuments') }}">Monuments</a>
											<a class="dropdown-item" href="{{ path('fetch2') }}">Guides</a>
										</div>
									</li>
									<li>


										{% if app.user %}
											<!-- Dropdown -->
											<li class="dropdown">
												<a class="dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
													Profile
												</a>
												{% if user.profilePicture %}
													<img src="{{ asset('uploads/profile_pictures/' ~ user.profilePicture) }}" alt="Profile Picture" class="rounded-circle" style="max-height: 28px; max-width: 28px">
												{% else %}
													<img src="{{ asset('img/noProfilePicture.png') }}" alt="Profile Picture" class="img-fluid rounded-circle" style="max-width: 28px; max-height: 28px;">
												{% endif %}
												<div class="dropdown-menu">
													<a class="dropdown-item" href="{{ path('app_profile') }}">
														Profile
														<i class="bi bi-person"></i>
													</a>
													<a class="dropdown-item" href="{{ path('Claims_add') }}">
														<i class="fa-solid fa-plus"></i>
														Claims
													</a>
													<a class="dropdown-item" href="{{ path('showSF') }}">
														<i class="fa-solid fa-subscript"></i>
														Subscriptions
													</a>
													<a class="dropdown-item" href="{{ path('addfb') }}">
														<i class="fa-solid fa-plus"></i>
														Feedback
													</a>
													<a class="dropdown-item" href="{{ path('favorites_index') }}">
														<i class="fa-solid fa-star"></i>
														Fav Accommodations
													</a>

													{% if is_granted('ROLE_ADMIN') %}
														<a class="dropdown-item" href="{{ path('app_back') }}">Backoffice</a>
													{% endif %}
													<a class="dropdown-item" href="{{ path('app_logout') }}">
														<i class="fa-solid fa-right-from-bracket"></i>
														Logout
													</a>
												</div>
											</li>
										{% else %}
											<li class="dropdown">
												<a class="dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
													SignIn/Up
												</a>
												<div class="dropdown-menu">
													<a class="dropdown-item" href="{{ path('app_login') }}">Sign In</a>
													<a class="dropdown-item" href="{{ path('app_register') }}">Sign Up</a>
												</div>
											</li>
										{% endif %}
									</li>
								</li>
							</ul>
						</div>
					</div>

				</nav>
				{% if isVerified %}
					<div class="alert alert-warning alert-dismissible fade show" role="alert">
						Please verify your account.
						<button type="button" class="close" data-dismiss="alert" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
				{% endif %}

			</header>
			<!-- End Header Area -->
		</body>
	</html>
